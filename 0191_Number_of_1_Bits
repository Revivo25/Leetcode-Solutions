class Solution(object):
    def hammingWeight(self, n):
        """
        :type n: int
        :rtype: int
        """
        ## 11 = 1011
        ##  1 = 0001
        ## 11 & 1 > 1011 & 0001 = 1
        ## n = n >> 1 > 1001 >> 1 > n = 100
        cnt = 0 
        while n > 0: 
            if n & 1 == 1:
                cnt+=1
            n = n >> 1
        return cnt
